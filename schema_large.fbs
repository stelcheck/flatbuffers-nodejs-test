namespace MyGame;

attribute "priority";

enum Color : byte { Red = 1, Green, Blue }

struct Vec3 {
	x:float;
	y:float;
	z:float;
}

union Any { Weapon, Pickup }

table Weapon {
	name: string;
	damage: short = 100;
}

table Pickup {
	name: string;
	damage: short = 200;
}

